  "scripts": {
    "start": "./bin/start-server",
    "start:cy": "CYPRESS=true ./bin/start-server",
    "build": "NODE_ENV=production webpack-cli --config webpack-prod.config.js",
    "test:cypress:open": "./cypress/node_modules/cypress/bin/cypress open",
    "test:cypress:run": "./cypress/node_modules/cypress/bin/cypress run",
    "test": "TZ=UTC jest",
    "test:watch": "TZ=UTC jest --watch",
    "test:debug": "TZ=UTC node --inspect ./node_modules/.bin/jest --runInBand",
    "lint:all-js": "eslint --ext=.jsx,.js,.tsx,.js,.json",
    "linf:all-js": "npm run lint:all-js -- --fix",
    "lint:ts": "eslint --quiet --ext=.tsx,.ts",
    "linf:ts": "npm run lint:ts -- --fix",
    "lint:json": "eslint --ext=.json",
    "lint:css:all": "stylelint --quiet \"app/**/*.css\"",
    "lint:css": "stylelint --quiet",
    "linf:css": "stylelint --quiet --fix",
    "ts:type-checking": "bash -c tsc",
    "bundle:analyzer": "NODE_ENV=production webpack --config webpack-analyzer.config.js",
    "storybook": "NODE_PATH=app APP_ENV=development start-storybook -s ./public -p 6006",
    "storybook:build": "build-storybook -s ./public",
    "chromatic": "npx chromatic"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(css|scss|md)": [
      "git add"
    ],
    "*.(css|scss)": [
      "npm run linf:css"
    ],
    "*.(js|jsx|ts|tsx|json)": [
      "npm run linf:all-js"
    ]
  },
